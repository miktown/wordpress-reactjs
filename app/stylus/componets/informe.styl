			color #c5c5c5
			margin-top 6em
			text-align center
		.sininforme
			color #c5c5c5
			// margin-left 11em
			// text-align left
		.sinDatos:before
			content ''
			background-image url('../../assets/sad.png')
			background-repeat no-repeat
			background-size: contain
			position absolute
			margin-bottom -10px
			margin-top 1px
			margin-left -1.9em
			height 35px
			width 35px
			z-index 0
		.sininforme:before
			content ''
			background-image url('../../assets/click.png')
			background-repeat no-repeat
			background-size: contain
			position absolute
			margin-bottom -10px
			margin-top 1px
			margin-left -1.9em
			marginleft -3em
			height 35px
			width 35px
			z-index 0
		.listaProfesores
			li
				padding .5em
				background white
				margin .3em 0
				box-shadow 0 1px 2px rgba(0, 0, 0, 0.22)